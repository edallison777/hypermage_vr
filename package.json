{
    "name": "hypermage-vr",
    "version": "0.1.0",
    "description": "Spec-driven multi-agent system for designing, implementing, and deploying Unreal Engine VR multiplayer levels",
    "private": true,
    "workspaces": [
        "Orchestrator",
        "Agents/*",
        "MCP/*"
    ],
    "scripts": {
        "setup:local": "npm install && npm run build",
        "build": "npm run build --workspaces",
        "test": "jest",
        "test:unit": "jest --testPathPattern=tests/unit",
        "test:properties": "jest --testPathPattern=tests/properties",
        "test:integration": "jest --testPathPattern=tests/integration",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint . --ext .ts,.tsx",
        "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
        "orchestrator:start": "npm run start --workspace=Orchestrator",
        "example:generate-level": "npm run example:generate-level --workspace=Orchestrator",
        "validate:schemas": "node scripts/validate-schemas.js"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^6.13.0",
        "@typescript-eslint/parser": "^6.13.0",
        "ajv": "^8.12.0",
        "ajv-formats": "^2.1.1",
        "eslint": "^8.54.0",
        "fast-check": "^3.15.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.0",
        "ts-jest": "^29.1.0",
        "typescript": "^5.3.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/edallison777/hypermage_vr.git"
    },
    "keywords": [
        "vr",
        "multiplayer",
        "unreal-engine",
        "meta-quest",
        "ai-agents",
        "strands",
        "aws",
        "gamelift"
    ],
    "author": "",
    "license": "UNLICENSED"
}